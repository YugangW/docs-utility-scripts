"regreplace.commands": [
// Last updated: August 10, 2022

// Substitute 2 spaces for each tab that does not precede a variable.

{"name": "6", "regexp": "\\t(?!<%= vars)", "replace": "  "},

// Search for errors everywhere

{"name": "10",  "regexp": "(-SERVICE-INSTANCE-NAME\\b[^`]*```\\s)", "replace": "$1<!--฿ Just |SERVICE-INSTANCE-NAME| is preferred. ฿-->"},
{"name": "11",  "regexp": "(-v\\s+\\d\\.\\d\\.\\d[\\n\\s.][^`]*```\\s)", "replace": "$1<!--฿ If this is a command to run and it has a version number, consider replacing the version number with an HBS variable, such as {{ vars.tap_version }}. ฿-->"},
{"name": "12",  "regexp": "(:(?=\\n\\n\\|\\s*Option))", "replace": "$1<!--฿ A sentence that introduces a table of options ends in a period, not a colon. ฿-->"},
{"name": "13",  "regexp": "(!\\[[^\\]]{0,44}\\]\\([^\\.]*\\.png\\))", "replace": "$1<!--฿ Alt text must describe the image in detail. ฿-->"},
{"name": "14",  "regexp": "(!\\[[^\\]]{141,}\\]\\([^\\.]*\\.png\\))", "replace": "$1<!--฿ Reduce alt text to 140 characters or fewer. ฿-->"},
{"name": "15",  "regexp": "(!\\[[^\\]]*[^\\.]]\\([^\\.]*\\.png\\))", "replace": "$1<!--฿ In alt text, use complete sentences that end with periods. ฿-->"},
{"name": "16",  "regexp": "((NOTE:|IMPORTANT:|CAUTION:))", "replace": "$1<!--฿ Proper note formatting is |> **Note:**| or |> **Caution:**| or |> **Important:**|. ฿-->"},
{"name": "17",  "regexp": "([\\*\\+\\-]\\s`[A-Za-z]*_)", "replace": "$1<!--฿ Use dashes for spacing in placeholders, not underscores. ฿-->"},
{"name": "18",  "regexp": "([\\s\\n]+\\*\\*[^*]{96})", "replace": "$1<!--฿ Consider reducing this run-in heading to fewer than 100 characters. ฿-->"},
{"name": "19",  "regexp": "([^!]\\[[a-z]+\\s[^\\]]*\\]\\([^\\)]*\\))", "replace": "$1<!--฿ The link should likely be a title or |THIRD-PARTY-NAME documentation|. ฿-->"},
{"name": "20",  "regexp": "([^\\>][^\\>]\\*\\*(Note:|Important:|Caution:))", "replace": "$1<!--฿ Proper note formatting is |> **Note:**| or |> **Caution:**| or |> **Important:**|. ฿-->"},
{"name": "21",  "regexp": "(\\.[a-z]+#[a-z-]+-\\d+\\))", "replace": "$1<!--฿ Find the anchor this link points to and give it a short HTML anchor ID, such as |anchor-id| -- keep it under 25 chars and use dashes instead of spaces -- then change this link to point to that HTML anchor ID. ฿-->"},
{"name": "22",  "regexp": "(\\.\\/[a-z-]*#[a-z-]*\\.html)", "replace": "$1<!--฿ Broken link -- the correct syntax is |page-name.html#anchor-id|. ฿-->"},
{"name": "23",  "regexp": "(\\.yml)", "replace": "$1<!--฿ Per YAML.org, |.yaml| is the preferred extension. If the reader creates this YAML file, fix every instance of the filename in every topic as part of your commit. Files with the |.yml| extension downloaded from elsewhere are outside our control. ฿-->"},
{"name": "24",  "regexp": "(\\[(?!.*\\n?.*AWS documentation).*\\]\\(https:..docs.aws.amazon.com.*\\))", "replace": "$1<!--฿ The link name must be |AWS documentation|. ฿-->"},
{"name": "25",  "regexp": "(\\[(?!.*\\n?.*BOSH documentation).*\\]\\(https:..bosh.io.*\\))", "replace": "$1<!--฿ The link name must be |BOSH documentation|. ฿-->"},
{"name": "26",  "regexp": "(\\[(?!.*\\n?.*cert-manager documentation).*\\]\\(https:..cert-manager.io.docs.*\\))", "replace": "$1<!--฿ The link name must be |cert-manager documentation|. ฿-->"},
{"name": "27",  "regexp": "(\\[(?!.*\\n?.*Cloud Foundry documentation).*\\]\\(https:..docs.cloudfoundry.org.*\\))", "replace": "$1<!--฿ The link name must be |Cloud Foundry documentation|. ฿-->"},
{"name": "28",  "regexp": "(\\[(?!.*\\n?.*Concourse documentation).*\\]\\(https:..concourse-ci.org.*\\))", "replace": "$1<!--฿ The link name must be |Concourse documentation|. ฿-->"},
{"name": "29",  "regexp": "(\\[(?!.*\\n?.*Docker documentation).*\\]\\(https:..docs.docker.com.*\\))", "replace": "$1<!--฿ The link name must be |Docker documentation|. ฿-->"},
{"name": "30",  "regexp": "(\\[(?!.*\\n?.*GCP documentation).*\\]\\(https:..cloud.google.com.*\\))", "replace": "$1<!--฿ The link name must be |GCP documentation|. ฿-->"},
{"name": "31",  "regexp": "(\\[(?!.*\\n?.*Istio documentation).*\\]\\(https:..istio.io.latest.docs.*\\))", "replace": "$1<!--฿ The link name must be |Istio documentation|. ฿-->"},
{"name": "32",  "regexp": "(\\[(?!.*\\n?.*Kubernetes documentation).*\\]\\(https:..kubernetes.io.docs.*\\))", "replace": "$1<!--฿ The link name must be |Kubernetes documentation|. ฿-->"},
{"name": "33",  "regexp": "(\\[(?!.*\\n?.*Microsoft documentation).*\\]\\(https:..docs.microsoft.com.*\\))", "replace": "$1<!--฿ The link name must be |Microsoft documentation|. ฿-->"},
{"name": "34",  "regexp": "(\\[(?!.*\\n?.*Percona documentation).*\\]\\(https:..www.percona.com.doc.*\\))", "replace": "$1<!--฿ The link name must be |Percona documentation|. ฿-->"},
{"name": "35",  "regexp": "(\\[(?!.*\\n?.*RabbitMQ documentation).*\\]\\(https:..www.rabbitmq.com.*\\))", "replace": "$1<!--฿ The link name must be |RabbitMQ documentation|. ฿-->"},
{"name": "36",  "regexp": "(\\[(?!.*\\n?.*Terraform documentation).*\\]\\(https:..www.terraform.io.docs.*\\))", "replace": "$1<!--฿ The link name must be |Terraform documentation|. ฿-->"},
{"name": "37",  "regexp": "(\\[(?!.*\\n?.*VMware Tanzu Support knowledge base).*\\]\\(https:..community.pivotal.io.*\\))", "replace": "$1<!--฿ The link name must be |VMware Tanzu Support documentation|. ฿-->"},
{"name": "38",  "regexp": "(\\].(?=\\((http|#)))", "replace": "$1<!--฿ Broken link ฿-->"},
{"name": "39",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.html[#a-z-]*\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
{"name": "40",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.md[#a-z-]*\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
{"name": "41",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.md\\.hbs[#a-z-]*\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
{"name": "42",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.png\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
{"name": "43",  "regexp": "(\\*\\*(Note|Important|Caution)\\*\\*)", "replace": "$1<!--฿ Proper note formatting is |> **Note:**| or |> **Caution:**| or |> **Important:**|. ฿-->"},
{"name": "44",  "regexp": "(\\*\\*[\\s\\n]+is[\\s\\n]+inactive\\b)", "replace": "$1<!--฿ |dimmed| or |unavailable| is the preferred term for a greyed-out UI object. ฿-->"},
{"name": "45",  "regexp": "(\\*\\*Warning)", "replace": "$1<!--฿ |Caution| is preferred over |Warning|. ฿-->"},
{"name": "46",  "regexp": "(\\<a\\s+id..[^\\w-]{26,})", "replace": "$1<!--฿ Shorten the anchor ID to 25 characters or fewer. Use dashes instead of spaces or periods. ฿-->"},
{"name": "47",  "regexp": "(\\<a\\s+id..[^\\w-]+\\.)", "replace": "$1<!--฿ Use dashes instead of spaces or periods in anchor IDs. ฿-->"},
{"name": "48",  "regexp": "(\\<a\\s+id..[^\\w-]+\\s)", "replace": "$1<!--฿ Use dashes instead of spaces or periods in anchor IDs. ฿-->"},
{"name": "49",  "regexp": "(\\<code\\>kubectl\\<\\/code\\>)", "replace": "$1<!--฿ Do not format |kubectl| as code. ฿-->"},
{"name": "50",  "regexp": "(\\<h1\\>)", "replace": "$1<!--฿ H1 headers are not allowed within topics. ฿-->"},
{"name": "51",  "regexp": "(\\<td\\>\\<\\/td\\>)", "replace": "$1<!--฿ Insert |n/a| in italics within purposefully empty cells. ฿-->"},
{"name": "52",  "regexp": "(\\<td\\>\\s*n\\/a)", "replace": "$1<!--฿ Insert |n/a| in italics within purposefully empty cells. ฿-->"},
{"name": "53",  "regexp": "(\\>\\s*(Note|Caution|Important))", "replace": "$1<!--฿ Proper note formatting is |> **Note:**| or |> **Caution:**| or |> **Important:**|. ฿-->"},
{"name": "54",  "regexp": "(\\|(?!(\\n))\\s*\\|)", "replace": "$1<!--฿ Insert |_n/a_| within purposefully empty cells. ฿-->"},
{"name": "55",  "regexp": "(\\|(?!(\\n))\\s*n\\/a)", "replace": "$1<!--฿ Insert |_n/a_| within purposefully empty cells. ฿-->"},
{"name": "56",  "regexp": "(\\|(?=\\n\\n+\\|[^\\|]*\\|))", "replace": "$1<!--฿ Do not stack tables. ฿-->"},
{"name": "57",  "regexp": "(\\|\\s*.*\\.\\.\\..*\\|[\\s\\n]+\\|-+\\|)", "replace": "$1<!--฿ Do not use ellipsis in a column header. ฿-->"},
{"name": "58",  "regexp": "(\\bAPP(?!-NAME)[^`]*```\\s)", "replace": "$1<!--฿ |APP-NAME| is preferred. ฿-->"},
{"name": "59",  "regexp": "(\\bchoose[\\s\\n]+\\*\\*)", "replace": "$1<!--฿ |select| or |click| is preferred for interacting with UI elements. ฿-->"},
{"name": "60",  "regexp": "(\\bChoose[\\s\\n]+\\*\\*)", "replace": "$1<!--฿ |Select| or |Click| is preferred for interacting with UI elements. ฿-->"},
{"name": "61",  "regexp": "(\\bnavigate[\\s\\n]+to(?=[\\s\\n]+\\[[^\\]]+\\]\\())", "replace": "$1<!--฿ |go to| is preferred for URLs. ฿-->"},
{"name": "62",  "regexp": "(\\bNavigate[\\s\\n]+to(?=[\\s\\n]+\\[[^\\]]+\\]\\())", "replace": "$1<!--฿ |Go to| is preferred for URLs. ฿-->"},
{"name": "63",  "regexp": "(\\bPRIVATE-KEY-FILE\\b[^`]*```\\s)", "replace": "$1<!--฿ |PATH-TO-PRIVATE-KEY| is preferred. ฿-->"},
{"name": "64",  "regexp": "(\\brun:[\\s\\n]*\\<code\\>[\\s\\n\\~]*\\$)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
{"name": "65",  "regexp": "(\\brun:[\\s\\n]*`+[\\s\\n\\~]*\\$[^`]*```\\s)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
{"name": "66",  "regexp": "(\\brun[^:]*:[\\s\\n]*\\<pre class)", "replace": "$1<!--฿ Commands are formatted in triple backtick blocks, not terminal snippets. ฿-->"},
{"name": "67",  "regexp": "(\\brunning[^:]*:[\\s\\n]*\\<code\\>[\\s\\n\\~]*\\$)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
{"name": "68",  "regexp": "(\\brunning[^:]*:[\\s\\n]*`+[\\s\\n\\~]*\\$)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
{"name": "69",  "regexp": "(\\bSERVICE-INSTANCE[\\s\\n]+[^`]*```\\s)", "replace": "$1<!--฿ |SERVICE-INSTANCE-NAME| is preferred. ฿-->"},
{"name": "70",  "regexp": "(\\btype(?=[\\s\\n]+`))", "replace": "$1<!--฿ Use |enter| when the user input appears on the screen. Use |run| when you want users to run commands. ฿-->"},
{"name": "71",  "regexp": "(\\bType(?=[\\s\\n]+`))", "replace": "$1<!--฿ Use |enter| when the user input appears on the screen. Use |run| when you want users to run commands. ฿-->"},
{"name": "72",  "regexp": "(\\bYOUR-PRIVATE-KEY\\b[^`]*```\\s)", "replace": "$1<!--฿ |PATH-TO-PRIVATE-KEY| is preferred. ฿-->"},
{"name": "73",  "regexp": "(\\bYOUR-VM\\b[^`]*```\\s)", "replace": "$1<!--฿ Specify the VM, as in |OPS-MANAGER-VM| or |STEMCELL-BASE-VM|. ฿-->"},
{"name": "74",  "regexp": "(\\n```[^`]*[A-Z][A-Z]+[\\-\\_][^`]*```\\s(?![\\s\\n]*Where))", "replace": "$1<!--฿ Verify that no placeholders above require explanation in the style of |Where PLACEHOLDER is...| ฿-->"},
{"name": "75",  "regexp": "(\\sCVE-\\d{4}(?!.*\\n?([^\\[]*\\]|[^\\>]*\\<\\/a\\>)))", "replace": "$1<!--฿ Link to the CVE page at https://nvd.nist.gov/ or, if unavailable, cve.org ฿-->"},
{"name": "76",  "regexp": "(##+[^\\n]*\\>\\s*[A-Z][a-z-]+ing\\b.*\\n\\n(.+\\n){1,}\\n+\\d\\.)", "replace": "$1<!--฿ Procedure headers use imperatives, not gerunds. ฿-->"},
{"name": "77",  "regexp": "(##+\\s*\\d\\.)", "replace": "$1<!--฿ Do not format a numbered step as a header. ฿-->"},
{"name": "78",  "regexp": "(`.*[_\\A-Z].*`[\\s\\n]+errand\\b)", "replace": "$1<!--฿ Errands tend to be lowercase and they use dashes instead of underscores or spaces. ฿-->"},
{"name": "79",  "regexp": "(`.*[_\\A-Z].*`[\\s\\n]+errands\\b)", "replace": "$1<!--฿ Errands tend to be lowercase and they use dashes instead of underscores or spaces. ฿-->"},
{"name": "80",  "regexp": "(`[^`]*\\s{{\\s[^`]*`)", "replace": "$1<!--฿ To escape HBS within a code block, add a backslash before the opening curly braces only. ฿-->"},
{"name": "81",  "regexp": "(```[^<]*(<strong>|<em>|<h\\d>|<html>|<body>)(?![^<]*<\\/pre>)[^`]*```\\s)", "replace": "$1<!--฿ Use unicode for angle brackets to ensure they show up in the snippet. |&lt;| for a left angle bracket and |&gt;| for a right angle bracket. ฿-->"},
{"name": "82",  "regexp": "(`kubectl`)", "replace": "$1<!--฿ Do not format |kubectl| as code. ฿-->"},
{"name": "83",  "regexp": "(`tanzu`[\\s\\n]+CLI\\b)", "replace": "$1<!--฿ |Tanzu CLI| is preferred. No code tags. ฿-->"},
{"name": "84",  "regexp": "(^[\\s]*(?!\\<li\\>)[^\\<]*\\<a href=.#[^\\>]*.>(?![^\\.]*\\n?[^\\.]*(earlier|previously|before|later)))", "replace": "$1<!--฿ After giving the anchor, state whether the target is |earlier| or |later| in the topic. ฿-->"},
{"name": "85",  "regexp": "(^\\[[^\\]]*\\]\\(#[^\\)]*\\)(?![^\\.]*\\n?[^\\.]*(earlier|previously|before|later)))", "replace": "$1<!--฿ After giving the anchor, state whether the target is |earlier| or |later| in the topic. ฿-->"},
{"name": "86",  "regexp": "(^\\*\\*(Note:|Important:|Caution:))", "replace": "$1<!--฿ Proper note formatting is |> **Note:**| or |> **Caution:**| or |> **Important:**|. ฿-->"},
{"name": "87",  "regexp": "(^\\d\\.\\s+.*example.*\\.[\\s\\n]*(?=```))", "replace": "$1<!--฿ When introducing an example in a step, end with a colon. ฿-->"},
{"name": "88",  "regexp": "(^\\d\\.\\s+[^:]*:\\s*\\n\\s+\\*)", "replace": "$1<!--฿ A step that has sub-steps ends in a period, not a colon. ฿-->"},
{"name": "89",  "regexp": "(^#\\s*(?=<a\\s+id))", "replace": "$1<!--฿ Do not use anchor IDs for level 1 headers in DocsWorks content. ฿-->"},
{"name": "90",  "regexp": "(^#\\s+[^#]*\\n(\\n[\\*\\+-]\\s+\\[[^\\]]+\\]\\([^\\)]+\\)){2,})", "replace": "$1<!--฿ If this is a topic ToC, maybe delete it. It is work to maintain it, and the jumplist in the subnav is obvious. ฿-->"},
{"name": "91",  "regexp": "(^##+\\s*.*\\n+\\|[^\\|]*\\|)", "replace": "$1<!--฿ Do not place a table immediately beneath a header. ฿-->"},
{"name": "92",  "regexp": "(^##+\\s*.*\\n+>\\s*\\*\\*)", "replace": "$1<!--฿ Do not place a note immediately beneath a header. ฿-->"},
{"name": "93",  "regexp": "(^##+\\s*(?!<a[\\s\\n]+id=)[^\\s#])", "replace": "$1<!--฿ Add an HTML anchor ID unless you have reason not to. Keep it under 26 chars. Use dashes instead of spaces and periods. ฿-->"},
{"name": "94",  "regexp": "(^#+\\s*[a-z][^\\d])", "replace": "$1<!--฿ Headers start with an uppercase letter. ฿-->"},
{"name": "95",  "regexp": "(^#+\\s*[A-Za-z-\\s]+\\s+[A-Z][a-z-]+$)", "replace": "$1<!--฿ Write all headers in sentence case. ฿-->"},
{"name": "96",  "regexp": "(^#+\\s*\\<a[\\s\\n]+id=[^>]*\\>\\<.a\\>\\s*[a-z])", "replace": "$1<!--฿ Headers start with an uppercase letter. ฿-->"},
{"name": "97",  "regexp": "(^#s*\\<a[\\s\\n]+id=[^>]*\\>)", "replace": "$1<!--฿ Do not use anchor IDs for level 1 headers in DocsWorks content. ฿-->"},
{"name": "98",  "regexp": "(<[A-Z]+_[A-Z_]+>[^`]*```\\s)", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of underscores. ฿-->"},
{"name": "99",  "regexp": "(<[A-Z]+-[A-Z_]+>[^`]*```\\s)", "replace": "$1<!--฿ If a placeholder, delete the angle brackets. ฿-->"},
{"name": "100",  "regexp": "(<[A-Z]+\\\\\\s[A-Z_]+>[^`]*```\\s)", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of escaped spaces. ฿-->"},
{"name": "101",  "regexp": "(<[A-Z]+\\s[A-Z_]+>[^`]*```\\s)", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of spaces. ฿-->"},
{"name": "102",  "regexp": "(docs-pcf-staging.cfapps.io[^\\s]*)", "replace": "$1<!--฿ Do not link to the staging version unless A. this is the main branch and B. there is no prod link for this page right now. ฿-->"},
{"name": "103",  "regexp": "(html#[a-z-]*\\)[\\s\\n]+earlier\\b)", "replace": "$1<!--฿ Delete |earlier|: the reference is in another topic. ฿-->"},
{"name": "104",  "regexp": "(html#[a-z-]*\\)[\\s\\n]+later\\b)", "replace": "$1<!--฿ Delete |later|: the reference is in another topic. ฿-->"},
{"name": "105",  "regexp": "(https:..docs-staging[^\\s]*)", "replace": "$1<!--฿ Do not link to the staging version unless A. this is the main branch and B. there is no prod link for this page right now. ฿-->"},
{"name": "106",  "regexp": "(PATH-TO-MANIFEST[^\\.][^`]*```\\s)", "replace": "$1<!--฿ Include the file extension, as in |PATH-TO-MANIFEST.yaml|. ฿-->"},
{"name": "107",  "regexp": "(PATH-TO-VARIABLES[^\\.][^`]*```\\s)", "replace": "$1<!--฿ Include the file extension, as in |PATH-TO-VARIABLES.yaml|. ฿-->"},
{"name": "108",  "regexp": "(Where `[A-Za-z]*_[^`]*`)", "replace": "$1<!--฿ Use dashes for spacing in placeholders, not underscores.  ฿-->"},

// Search for errors outside code tags

{"name": "112",  "regexp": "(---\\n\\n[^#\\n]*(?=(#[^#]|###))(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ The first header must be ## -- no smaller, no larger. ฿-->"},
{"name": "113",  "regexp": "(,(?=[\\s\\n]+then[^\\.]*\\.\\n)(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ Consider deleting |then| or writing |and| after the comma. ฿-->"},
{"name": "114",  "regexp": "([\\s\\.]\\d+\\-\\d+(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ Use &ndash; for a range, not a regular dash. ฿-->"},
{"name": "115",  "regexp": "([\\s\\n]'[^'\\n]+'(?!.*%>)(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ Double quotation marks are preferred in US English. ฿-->"},
{"name": "116",  "regexp": "([\\s\\n]+--[\\s\\n]+(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ IX Standards forbids em dashes. ฿-->"},
{"name": "117",  "regexp": "([\\s\\n]+(?!\\ba\\b|\\bany\\b|\\bthat\\b|\\bthe\\b|\\bthis\\b)[a-z-]*[\\s\\n]+shutdown\\b(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ The noun is |shutdown|. The action is |shut down|. ฿-->"},
{"name": "118",  "regexp": "(\\b[a-z-]+[\\s\\n]+Architect\\b(?![^`]*`[\\s\\.,:]|[^`]*```[^\\w]|[^<]*<\\/code\\>|[^\\*:]*\\*\\*[\\s\\n-\\.,]|[^฿]*฿-->|[^\\[]*\\]|[^\\s]*\\)|[^\\.\\s]*\\.png|[^\\(]*\\)|[^\\s]*\\>))", "replace": "$1<!--฿ |architect| is a common noun, so lowercase is preferred. ฿-->"},
